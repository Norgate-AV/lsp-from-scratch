{
    "name": "REPLACE_ME",
    "description": "REPLACE_ME",
    "author": "REPLACE_ME",
    "license": "MIT",
    "version": "0.0.1",
    "publisher": "REPLACE_ME",
    "repository": {
        "type": "git",
        "url": "REPLACE_ME"
    },
    "engines": {
        "vscode": "^1.75.0"
    },
    "dependencies": {
        "vscode-languageclient": "^8.1.0"
    },
    "devDependencies": {
        "@types/vscode": "^1.81.0"
    },
    "contributes": {
        "languages": [
			{
				"id": "netlinx-source",
				"aliases": [
					"AMX Netlinx Source"
				],
				"extensions": [
					".axs",
					".axi"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "netlinx-library",
				"aliases": [
					"AMX Netlinx Library"
				],
				"extensions": [
					".axb",
					".lib"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "netlinx-source",
				"scopeName": "source.netlinx",
				"path": "./syntaxes/amx-netlinx.tmLanguage.json"
			},
			{
				"language": "netlinx-library",
				"scopeName": "source.netlinx",
				"path": "./syntaxes/amx-netlinx.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "netlinx-source",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "netlinx-library",
				"path": "./snippets/snippets.json"
			}
		]
    }
}
